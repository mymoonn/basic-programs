a<!DOCTYPE html>
<html>
    <head>
       
    </head>
<script>
    function factorial(n) {
        let f= 1;
        if (n == 0 || n == 1) {
            return f;
        } else {
            for (let i = n; i >= 1; i--) {
                f = f * i;
            }
            return f;
        }
    }
    let n = prompt("enter a number");
    f = factorial(n)
    console.log("The factorial of  the number is " + n + " is " + f);
    </script>
    

</html >
